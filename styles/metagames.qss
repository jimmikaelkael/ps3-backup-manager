    dialogbuttonbox-buttons-have-icons: true;
    dialog-ok-icon: url(styles/images/check.png);
    dialog-cancel-icon: url(styles/images/cancel.png);
}*/

QHeaderView::section {
    color: #dcdcdc;
    background-color: #202020;
}

QFileDialog {
    color: #dcdcdc;
    background-color:#0f0f0f;
}

QComboBox { 
    color: #dcdcdc;
    background-color: #171717;
    border:1px solid #2c2c2c;
}
QComboBox::item:selected { 
    color: #dcdcdc;
    background-color: #654321;
}
QComboBox::!read-only {
    color: #dcdcdc;
    padding-left: 5px;
}
QComboBox::read-only {
    color: #dcdcdc;
    padding-left: 5px;
}

QCheckBox {
    color: #dcdcdc;
    background-color: transparent;
    spacing: 10px;
}
QCheckBox::indicator {
    width: 12px;
    height: 12px;
    background-color: #171717;
    border:1px solid #2c2c2c;
}
QCheckBox::indicator::checked {
    image: url("styles/images/checkmark.png")
}

QListView {
    color: #dcdcdc;
    background-color: #171717;
    border:1px solid #2c2c2c;
}
QListView::item {
    background-color: #171717;
}
QListView::item:selected {
    background-color:  #dd7200;
}

QSizeGrip {
    background-color: transparent;
}

